{
    "name": "silverstripe/image-srcset",
    "description": "Silverstripe CMS module for controlling srcset when rendering images",
    "type": "silverstripe-vendormodule",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "srcset"
    ],
    "authors": [
        {
            "name": "SilverStripe",
            "homepage": "http://silverstripe.com"
        },
        {
            "name": "The SilverStripe Community",
            "homepage": "http://silverstripe.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "silverstripe/assets": "^2.3",
        "silverstripe/vendor-plugin": "^2"
    },
    "require-dev": {
        "silverstripe/recipe-testing": "^3",
        "squizlabs/php_codesniffer": "^3.7",
        "silverstripe/standards": "^1"
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\ImageSrcset\\": "src/",
            "SilverStripe\\ImageSrcset\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "phpcs src/ tests/php/",
        "lint-clean": "phpcbf src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
